void main() {
   
  //Agregado el constructor, al crear el objeto se pide que se definan los valores de los atributos
  final auto1 = new Auto('Mazda','abc123');
   
   //impresión de objeto de clase, ya con el método sobreescrito
  print(auto1);
  
}
 
class Auto {
   
  //Declaración de atributos de una clase
  String marca='';
  String placa=' '; 
   
  //Constructor
  Auto(String marca, String placa){
    this.marca = marca;
    this.placa = placa;
  }
   
  //Sobreesctirura del método
  String toString(){
    return '${this.marca} - ${this.placa}';
  }
}